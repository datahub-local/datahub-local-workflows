    {
      "parameters": {
        "jsCode": "const priorityOrder = {\n  HIGH: 0,\n  MEDIUM: 1,\n  LOW: 2\n};\n\nconst filteredEntries = $input.all().map(item=>item.json).filter(item => item.URL && /^(QUEUE)$/.test(item.STATUS)).toSorted((a, b) => {\n  const aPriority = priorityOrder[a.PRIORITY] ?? 3;\n  const bPriority = priorityOrder[b.PRIORITY] ?? 3;\n\n  if (aPriority !== bPriority) {\n    return aPriority - bPriority; // sort by priority\n  }\n\n  return a.row_number - b.row_number; // sort by row_number ascending\n});\n\n//console.log(filteredEntries.slice(0))\n\nreturn filteredEntries.slice(0, 1);"
      },

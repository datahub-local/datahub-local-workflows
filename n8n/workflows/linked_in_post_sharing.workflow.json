    {
      "parameters": {
        "jsCode": "const timeThreshold = 10 * 24 * 60 * 60 * 1000;\n\nconst now = new Date();\nconst thresholdDate = new Date(now.getTime() - timeThreshold);\n\nconst filteredEntries = $input.all().map(item=>item.json)\n  .filter(item => item.URL && /^(IN_PROGRESS|WAITING_APPROVAL)$/.test(item.STATUS))\n  .filter(item => {\n    if (!item.UPDATE_DATE) return false;\n    const updateDate = new Date(item.UPDATE_DATE);\n    return !isNaN(updateDate) && updateDate < thresholdDate;\n  })\n\n//console.log(filteredEntries)\n\nreturn filteredEntries;"
      },
